{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/((?!api/|_next/|static/|assets/|favicon.ico|sw.js|src/|node_modules/|@vite|@react-refresh|.*\\..*).*)",
      "destination": "/index.html"
    }
  ]
}